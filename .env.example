# Environment Variables for CMS Data Agent
# Copy this file to .env and add your actual values

# Claude API Configuration
CLAUDE_API_KEY=your_claude_api_key_here
CLAUDE_MODEL=claude-3-sonnet-20240229
CLAUDE_MAX_TOKENS=1024

# Rate Limiting
RATE_LIMIT_REQUESTS_PER_MINUTE=10
RATE_LIMIT_REQUESTS_PER_HOUR=100
RATE_LIMIT_REQUESTS_PER_DAY=500

# Optional: Additional API Keys
OPENAI_API_KEY=your_openai_key_here
ANTHROPIC_API_KEY=your_anthropic_key_here

# Security
ALLOWED_ORIGINS=http://localhost:3000,https://yourusername.github.io
JWT_SECRET=your_jwt_secret_here

# Database (optional)
DATABASE_URL=sqlite:///cms_agent.db
REDIS_URL=redis://localhost:6379
